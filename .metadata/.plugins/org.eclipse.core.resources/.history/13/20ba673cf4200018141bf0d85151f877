
public class Person {

	private String name=null;
	private int age=0;

	public Person(String name, int age){
		this.name = name;
		this.age = age;
	}
    public Person() {
		// TODO 自動生成されたコンストラクター・スタブ
	}
	public String getName(){
     return this.name;
     return this.age
    }

    public void setName(String name){
    	this.name=name;
    }
}
